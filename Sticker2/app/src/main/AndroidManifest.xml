<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wirda61.wirdakurnia.sticker">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logochart2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logochart2"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.activities.ActivityDetail"></activity>
        <activity android:name=".activities.activities.MainActivity"></activity>
        <activity android:name=".activities.activities.ActivityHome"/>
        <activity android:name=".activities.activities.ActivityAdd">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.activities.ActivitySetting">
        </activity>
    </application>

</manifest>